generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Message {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  text       String?
  image      String?
  createdAt DateTime @db.Date @default(now()) @map("created_at")
}
